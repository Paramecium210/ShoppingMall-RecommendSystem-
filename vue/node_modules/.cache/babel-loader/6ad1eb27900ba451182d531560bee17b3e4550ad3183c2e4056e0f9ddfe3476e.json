      <el-form :model=\"form\" label-width=\"100px\" style=\"padding-right: 50px\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item label=\"username\" prop=\"username\">\r\n          <el-input v-model=\"form.username\" placeholder=\"用户名\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"姓名\" prop=\"name\">\r\n          <el-input v-model=\"form.name\" placeholder=\"姓名\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"电话\" prop=\"phone\">\r\n          <el-input v-model=\"form.phone\" placeholder=\"电话\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"form.email\" placeholder=\"邮箱\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"formVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      addressData: [],\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      formVisible : false,\r\n      form: {},\r\n      rules: {\r\n        username: [\r\n          {required: true, message: 'Enter your account', trigger: 'blur'},\r\n        ]\r\n      },\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadAddress(1)\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    addAddress() {\r\n      this.form = {}\r\n      this.formVisible = true\r\n    },\r\n    del (id) {\r\n      this.$request.delete('/address/delete/' + id).then(res => {\r\n        if (res.code === '200'){\r\n          this.$message.success('Delete successfully')\r\n          this.loadAddress(1)\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.loadAddress(pageNum)\r\n    },\r\n    loadAddress(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/address/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        if (res.code === '200'){\r\n          this.addressData = res.data?.list\r\n          this.total = res.data?.total\r\n        }else {\r\n          this.$message.error(res.msg)\r\n        }\r\n\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAoEA;EAEAA,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;MACAC,KAAA;QACAC,QAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,WAAA;EACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAT,IAAA;MACA,KAAAD,WAAA;IACA;IACAW,IAAAC,EAAA;MACA,KAAAC,QAAA,CAAAC,MAAA,sBAAAF,EAAA,EAAAG,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAX,WAAA;QACA;UACA,KAAAU,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;IACA;IACAC,oBAAAzB,OAAA;MACA,KAAAW,WAAA,CAAAX,OAAA;IACA;IACAW,YAAAX,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAgB,QAAA,CAAAU,GAAA;QACAC,MAAA;UACA3B,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAiB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAArB,WAAA,GAAAoB,GAAA,CAAA1B,IAAA,EAAAmC,IAAA;UACA,KAAA1B,KAAA,GAAAiB,GAAA,CAAA1B,IAAA,EAAAS,KAAA;QACA;UACA,KAAAmB,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MAEA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}