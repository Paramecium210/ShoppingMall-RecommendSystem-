{"ast":null,"code":"export default {\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      addressData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {},\n      rules: {\n        username: [{\n          required: true,\n          message: 'Enter your account',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  mounted() {\n    this.loadAddress(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    addAddress() {\n      this.form = {};\n      this.formVisible = true;\n    },\n    del(id) {\n      this.$request.delete('/address/delete/' + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success('Delete successfully');\n          this.loadAddress(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.loadAddress(pageNum);\n    },\n    loadAddress(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/address/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.addressData = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","addressData","pageNum","pageSize","total","formVisible","form","rules","username","required","message","trigger","mounted","loadAddress","methods","addAddress","del","id","$request","delete","then","res","code","$message","success","error","msg","handleCurrentChange","get","params","list"],"sources":["src/views/front/Address.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\" width: 70%;